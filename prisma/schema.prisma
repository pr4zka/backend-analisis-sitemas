generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String 
  person   Person[]
}

model Person {
  id    Int    @id @default(autoincrement())
  name  String
  age   Int
  email String
  usuario  User   @relation(fields: [userId], references: [id])
  userId Int
}
